
<%= render 'shared/workoutgenerator'%>

<div class="container">
  <div class="col-md-2"></div>
  <div class="col-md-8">
  <%= form_for @workout do |f| %>
  <h1><%= f.text_field :name %></h1>
  <%= f.submit class: "btn btn-default" %>
  <% end %>
  <%= render 'displayworkout', exercises: @exercises %>
  </div>
  <div class="col-md-2"></div>
</div>
