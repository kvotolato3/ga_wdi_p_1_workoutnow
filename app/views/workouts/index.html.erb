<div class="container">
  <div class="col-md-2"></div>
  <div class="col-md-8">
    <h1>My saved workouts</h1>
      <ul>
      <% @workouts.each do |workout| %>
        <li><%= link_to workout.name, workout_path(workout) %> | Duration: <%= workout.duration %> minutes<br>
        </li>
      <% end %>
      </ul>
  </div>
  <div class="col-md-2"></div>
</div>
